<Window x:Class="WordSplitter.GUI.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        Title="MainWindow" Height="350" Width="525">
    <Grid>
        <Button Content="Count" Height="29" HorizontalAlignment="Left" Margin="418,36,0,0" Name="button_count" VerticalAlignment="Top" Width="75" Click="button_count_Click" />
        <TextBox Height="33" HorizontalAlignment="Left" Margin="21,36,0,0" Name="textBox_input" VerticalAlignment="Top" Width="389" />
        <ListBox Height="196" HorizontalAlignment="Left" Margin="21,99,0,0" Name="listBox_results" VerticalAlignment="Top" Width="472" ItemsSource="{Binding Path=Results}" />
        <TextBlock Height="18" HorizontalAlignment="Left" Margin="21,75,0,0" Name="text_results" Text="Results" VerticalAlignment="Top" Width="88" />
        <TextBlock Height="19" HorizontalAlignment="Left" Margin="22,18,0,0" Name="text_input" Text="Input" VerticalAlignment="Top" Width="113" />
    </Grid>
</Window>
